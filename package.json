  "dependencies": {
    "three": "^0.125.0"
  },
  "devDependencies": {
    "acorn": "^7.1.1",
    "brfs": "^2.0.2",
    "browserify": "^16.5.1",
    "babelify": "10.0.0",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
